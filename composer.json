{
    "name": "oas-php/utils",
    "description": "common code used across oas-php project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kuba Biernacki",
            "email": "kubiernacki@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-json": "*",
        "nikic/php-parser": "^4.10",
        "psr/event-dispatcher": "^1.0",
        "nikic/iter": "^2.0",
        "biera/array-accessor": "^1.0.1",
        "phpdocumentor/type-resolver": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "OAS\\Utils\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\OAS\\Utils\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
